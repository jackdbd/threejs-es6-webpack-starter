{
  "name": "threejs-es6-webpack-starter",
  "version": "3.0.1",
  "description": "A starter project for Three.js with support for ES6 and Webpack 4",
  "main": "./src/js/index.js",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.js --mode development --open",
    "build": "webpack --config webpack.config.js --progress --mode production",
    "lint": "prettier --write '{,!(node_modules)/}**/*.{js,jsx}'",
    "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save"
  },
  "repository": "git@github.com:jackdbd/threejs-es6-webpack-starter.git",
  "author": "jackdbd <jackdebidda@gmail.com> (http://www.giacomodebidda.com/)",
  "license": "MIT",
  "dependencies": {
    "orbit-controls-es6": "1.0.12",
    "three": "0.95.0"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-2": "6.24.1",
    "clean-webpack-plugin": "0.1.19",
    "compression-webpack-plugin": "1.1.11",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.7.0",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "4.3.1",
    "mini-css-extract-plugin": "0.4.0",
    "node-sass": "4.9.0",
    "prettier": "1.12.1",
    "sass-lint": "1.12.1",
    "sass-loader": "7.0.1",
    "style-loader": "0.21.0",
    "webpack": "4.7.0",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-cli": "3.0.0",
    "webpack-dev-server": "3.1.4",
    "webpack-glsl-loader": "1.0.1"
  },
  "keywords": [
    "threejs",
    "three.js",
    "boilerplate",
    "webpack",
    "es6",
    "babel",
    "sass"
  ],
  "bugs": {
    "url": "https://github.com/jackdbd/threejs-es6-webpack-starter/issues"
  },
  "homepage": "https://github.com/jackdbd/threejs-es6-webpack-starter"
}
